		{
			"id"			:	"{{g.id}}", 
			"name"			: 	"{{g.name}}", 
			"description"	:	"{{g.description}}" , 
			"moneyLimit"	:	"{{g.moneylimit}}"
		}
		
		{% else %}
		{
			"id"			:	"{{g.id}}", 
			"name"			: 	"{{g.name}}", 
			"description"	:	"{{g.description}}" , 
			"moneyLimit"	:	"{{g.moneylimit}}"
		},
		{% endif %}
	{% endfor %}
]