{
	"id":"{{s.id}}", 
	"title":"{{s.title}}", 
	"description":"{{s.description}}" , 
	"startTime":"{{s.startTime|date('Y-m-d H:i:s')}}" , 
	"endTime":"{{s.endTime|date('Y-m-d H:i:s')}}", 
	"requestDate":"{{s.requestdate|date('Y-m-d H:i:s')}}", 
	"type":"{{s.type}}",
	"client":"{{s.client.user.firstname ~" "~s.client.user.lastname}}",
	{% if s.worker!=null %}
		"worker":"{{s.worker.user.firstname~" "~s.worker.user.lastname}}",
	{% else %}
		"worker" : "NONE",
	{% endif %}
	"state":"{{s.state}}",
	{% if s.service!=null %}
		"service":"{{s.service.name}}"
	{% else %}
		"service": "NONE"
	{% endif %},
	"props":[
		{% for p in props %}
			{% if loop.last %}
			{	
				"id":"{{p.id}}",
				"service":"{{p.service.name}}",
				"organisation":"{{p.organisation.name}}",
				"decision":"{{p.decision}}"
			}
			{% else %}
			{	
				"id":"{{p.id}}",
				"service":"{{p.service.name}}",
				"organisation":"{{p.organisation.name}}",
				"decision":"{{p.decision}}"
			},
			{% endif %}
		{% endfor %}
	]
}