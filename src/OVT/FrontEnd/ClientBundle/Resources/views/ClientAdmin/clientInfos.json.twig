{
	"id":{{w.id}}, 
	"user": {
		"id":"{{w.user.id}}",
		"firstname":"{{w.user.firstname}}", 
		"lastname":"{{w.user.lastname}}", 
		"email":"{{w.user.email}}", 
		"address":"{{w.user.address}}", 
		"phoneNumber":"{{w.user.phoneNumber}}",
		"type":"{{w.user.type}}"
	},
	"equipments":"{{w.equipements}}",
	"langue":"{{w.language}}" , 
	{% if w.group!=null %}
		"group":"{{w.group.name}}"
	{% else %}
		"group": "NONE"
	{% endif %},
	"authorizedServices": [
	{% for s in w.authorizedServices %}
		{% if loop.last %}
		{
			"id":"{{s.id}}",
			"name":"{{s.name}}"
		}
		{% else %}
		{
			"id":"{{s.id}}",
			"name":"{{s.name}}"
		},
		{% endif %}
	{% endfor %}
	]
}