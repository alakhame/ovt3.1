{% for s in allS %}
	<div class="service_tab" onclick="toggleService({{s.id}},{{org.id}})"  style='display:inline-block'> 
		<span class="subtitle_color_text" >{{s.name~' : '}}</span>
	{% if s in authS %}
		<div class="valid_box select"> 
			<svg  version="1.1" baseProfile="full" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:ev="http://www.w3.org/2001/xml-events" viewBox="0 0 22 17">
				<use xlink:href="{{asset('svg/done_white.svg')}}#done_white"></use>
			</svg>
		</div>

	{% else %}
		<div class="valid_box"> 
			<svg  style="z-index:-10" version="1.1" baseProfile="full" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:ev="http://www.w3.org/2001/xml-events" viewBox="0 0 22 17">
				<use xlink:href="{{asset('svg/done_white.svg')}}#done_white"></use>
			</svg>
		</div>
	{% endif %} 
	</div> 
{% endfor %}